sourcefiles="${sourcefiles} ${binutilstar}" 

#gmpver=6.0.0a - requires renaming build dir
gmpver=5.1.3
gmpbase=gmp-${gmpver}
gmptar=${gmpbase}.tar.bz2
sourcefiles="${sourcefiles} ${gmptar}" 

mpfrver=3.1.2
mpfrbase=mpfr-${mpfrver}
mpfrtar=${mpfrbase}.tar.bz2
